{
  "name": "react-redux-graphql-apollo-bootstrap-webpack-starter",
  "version": "2.0.1",
  "description": "react js + redux + graphQL + Apollo + react router4 + hot reload + devTools + bootstrap + webpack 3 starter",
  "main": "src/index.js",
  "scripts": {
    "flow": "flow",
    "jest": "jest",
    "dev": "rimraf docs/assets && cross-env NODE_ENV=dev webpack --display-entrypoints --config webpack.dev.config.js",
    "prod": "rimraf docs/assets && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "analyze": "rimraf docs/assets && cross-env NODE_ENV=production webpack -p --config webpack.bundle-analyze.config.js",
    "start": "rimraf docs/assets && cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "start-spa-dev": "npm run dev && cross-env NODE_ENV=dev nodemon ./src/server/SPA/server.js",
    "start-spa-prod": "npm run prod && cross-env NODE_ENV=production node ./src/server/SPA/server.js",
    "build-ssr": "rimraf src/server/SSR/output && cross-env NODE_ENV=production  webpack --color --config webpack.server.ssr.config.js",
    "serve-ssr": "npm run prod && npm run build-ssr && cross-env NODE_ENV=production node ./src/server/SSR/output/server.js"
  },
  "engines": {
    "node": ">=8.1.4",
    "npm": "^5.6.0",
    "yarn": ">=1.3.2"
  },
  "keywords": [
    "react",
